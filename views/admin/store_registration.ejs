<%- include ('common/layout_top.ejs') %>


<!-- Page header -->
<div class="page-header">
    <div class="breadcrumb-line breadcrumb-line-light header-elements-lg-inline border-bottom">
        <div class="d-flex">
            <div class="breadcrumb">
                <a href="#" class="breadcrumb-item"><i class="icon-home2 mr-2"></i>홈</a>
                <span class="breadcrumb-item active">브랜드 및 매장 관리</span>
            </div>

            <a href="#" class="header-elements-toggle text-body d-lg-none"><i class="icon-more"></i></a>
        </div>
    </div>

    <div class="page-header-content header-elements-lg-inline">
        <div class="page-title d-flex">
            <h4><i class="icon-arrow-left52 mr-2"></i> <span class="font-weight-semibold">브랜드 및 매장 관리</span> - 매장 등록</h4>
            <a href="#" class="header-elements-toggle text-body d-lg-none"><i class="icon-more"></i></a>
        </div>
    </div>
</div>
<!-- /page header -->


<!-- Page content -->
<div class="page-content pt-0">
    <%- include ('common/side_nav.ejs') %>
    <!-- Main content -->
    <div class="content-wrapper">

        <!-- Content area -->
        <div class="content">

            <!-- Dashboard content -->
            <div class="row">
                <div class="col-lg-10 offset-lg-1">
                    <div class="card">
                        <div class="card-header">
                            <h5 class="card-title font-weight-bold">매장 등록</h5>
                        </div>

                        <div class="card-body">
                            <form action="#">
                                <div class="form-group">
                                    <label>브랜드 선택:</label>
                                    <select class="form-control form-control-select2" data-fouc>
                                        <option value="C" selected>브랜드 선택</option>
                                        <option value="CT">메르세데스 벤츠</option>
                                        <option value="DE">테슬라</option>
                                        <option value="FL">혼다</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>매장번호:</label>
                                    <input type="text" class="form-control" placeholder="매장번호를 입력해주세요.">
                                </div>
                                <div class="form-group">
                                    <label>지점명:</label>
                                    <input type="text" class="form-control" placeholder="지점명 입력해주세요.">
                                </div>
                                <div class="form-group">
                                    <label>주소:</label>
                                    <input type="text" class="form-control" placeholder="지점 주소를 입력해주세요.">
                                </div>
                                <div class="form-group">
                                    <label>연락처:</label>
                                    <input type="text" class="form-control" placeholder="지점 연락처를 입력해주세요.">
                                </div>
                                <div class="form-group">
                                    <label>아이디:</label>
                                    <input type="text" class="form-control" placeholder="매장에서 사용할 아이디를 입력해주세요.">
                                </div>
                                <div class="form-group">
                                    <label>비밀번호:</label>
                                    <input type="password" class="form-control" placeholder="비밀번호를 입력해주세요..">
                                </div>
                                <div class="form-group">
                                    <label>비밀번호 확인:</label>
                                    <input type="password" class="form-control" placeholder="비밀번호를 한번 더 입력해주세요..">
                                </div>
                                <div class="form-group">
                                    <label>승인자 선택:</label>
                                    <select class="form-control form-control-select2" data-fouc>
                                        <option value="C" selected>승인자 선택</option>
                                        <option value="CT">홍길동 사원</option>
                                        <option value="DE">김철수 대리</option>
                                        <option value="FL">이영희 과장</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label>입찰 담당자 선택:</label>
                                    <select class="form-control form-control-select2" data-fouc>
                                        <option value="C" selected>입찰 담당자 선택</option>
                                        <option value="CT">홍길동 사원</option>
                                        <option value="DE">김철수 대리</option>
                                        <option value="FL">이영희 과장</option>
                                    </select>
                                </div>


                                <div class="text-right">
                                    <button type="submit" class="btn btn-primary">매장 등록 <i class="icon-paperplane ml-2"></i></button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /content area -->

        </div>
        <!-- /main content -->

    </div>
    <!-- /page content -->



    <!-- /notifications -->
<%- include ('common/layout_bottom.ejs') %>